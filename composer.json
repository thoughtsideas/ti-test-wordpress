{
  "name": "thoughtsideasa/ti-test-wordpress",
  "type": "project",
  "license": "proprietary",
  "description": "",
  "homepage": "https://ti-test-wordpress.herokuapp.com/",
  "authors": [
    {
      "name": "EDGE Creative",
      "homepage": "https://www.edge-creative.com",
      "email": "info@edge-creative.com"
    }
  ],
  "keywords": [
    "wordpress",
    "composer",
    "wp"
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {}
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
 ],
  "require": {
    "php": "^7.2",
    "composer/installers": "~1.0",
    "ext-bcmath": "*",
    "ext-calendar": "*",
    "ext-exif": "*",
    "ext-gd": "*",
    "ext-gettext": "*",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "ext-pcntl": "*",
    "ext-shmop": "*",
    "ext-sodium": "*",
    "ext-xmlrpc": "*",
    "ext-xsl": "*",
    "humanmade/s3-uploads": "2.1.*",
    "inpsyde/wonolog": "1.0.*",
    "johnpbloch/wordpress": "5.5.*",
    "vlucas/phpdotenv": "3.3.*",
    "wpackagist-plugin/classic-editor":"1.5.*",
    "wpackagist-plugin/lj-maintenance-mode": "2.4.*",
    "wpackagist-plugin/nginx-cache": "1.0.*",
    "wpackagist-plugin/post-type-archive-links": "1.3.*",
    "wpackagist-plugin/redis-cache": "1.4.*",
    "wpackagist-plugin/safe-redirect-manager": "1.9.*",
    "wpackagist-plugin/secure-db-connection": "1.1.*",
    "wpackagist-plugin/safe-svg": "1.9.*",
    "wpackagist-plugin/sendgrid-email-delivery-simplified":"1.11.*",
    "wpackagist-plugin/stream": "3.4.*",
    "wpackagist-plugin/wordpress-seo":"11.8.*",
    "wpackagist-plugin/wp-help": "1.5.*",
    "wpackagist-plugin/wp-robots-txt": "1.1.*",
    "wpackagist-plugin/wp-smushit":"3.2.*",
    "wpackagist-theme/twentynineteen": "*",
    "wp-cli/wp-cli-bundle": "2.3.*"
    },
  "require-dev": {
    "heroku/heroku-buildpack-php": "*"
  },
  "config": {},
  "extra": {
    "wordpress-install-dir": "html/wordpress/",
    "installer-paths": {
      "html/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "html/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "html/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  }
}
